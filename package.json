{
  "name": "vue-fa",
  "version": "4.0.1",
  "description": "Tiny FontAwesome 5+ component for Vue.js",
  "keywords": [
    "fontawesome",
    "font-awesome",
    "vue",
    "vue.js",
    "component",
    "icon",
    "svg",
    "duotone"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cweili/vue-fa.git"
  },
  "author": "Cweili",
  "license": "MIT",
  "homepage": "https://cweili.github.io/vue-fa/",
  "bugs": {
    "url": "https://github.com/Cweili/vue-fa/issues"
  },
  "main": "dist/vue-fa.cjs.js",
  "module": "dist/vue-fa.esm.js",
  "unpkg": "dist/vue-fa.js",
  "jsdelivr": "dist/vue-fa.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "predev": "eslint --fix \"{src,test,docs,docs/v2}/*.{js,ts,vue}\"",
    "build:docs": "cd docs && rollup -c",
    "dev:build": "cd docs && rollup -c -w",
    "dev:serve": "sirv docs --dev",
    "dev": "run-p dev:build dev:serve",
    "test": "jest test --coverage",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@cweili/fa-test-util": "^1.0.0",
    "@fortawesome/fontawesome-common-types": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vue/compiler-sfc": "^3.2.9",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/server-renderer": "^3.2.9",
    "autoprefixer": "^10.3.4",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-csso": "^6.0.0",
    "pretty": "^2.0.0",
    "rollup": "^2.61.1",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-typescript2": "^0.32.1",
    "rollup-plugin-vue": "^6.0.0",
    "sirv-cli": "^2.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2",
    "vue": "^3.2.9",
    "vue-eslint-parser": "^9.0.3",
    "vue3-jest": "^27.0.0-alpha.1"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-common-types": "*",
    "vue": "^3.0.0"
  }
}
