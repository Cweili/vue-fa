{
  "name": "vue-fa",
  "version": "1.3.0",
  "description": "Tiny FontAwesome 5 component for Vue.js",
  "keywords": [
    "fontawesome",
    "font-awesome",
    "vue",
    "vue.js",
    "component",
    "icon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cweili/vue-fa.git"
  },
  "author": "Cweili",
  "license": "MIT",
  "homepage": "https://cweili.github.io/vue-fa/",
  "bugs": {
    "url": "https://github.com/Cweili/vue-fa/issues"
  },
  "main": "dist/vue-fa.js",
  "module": "dist/vue-fa.esm.js",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "postbuild": "eslint --fix *.js src/*.vue test/*.js examples/*.js examples/*.vue",
    "build:examples": "cd examples && rollup -c",
    "dev:build": "cd examples && rollup -c -w",
    "dev:serve": "sirv examples --dev",
    "dev": "run-p dev:build dev:serve",
    "test": "jest test --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-vue": "^3.0.0",
    "sirv-cli": "^0.4.2",
    "vue": "^2.6.10",
    "vue-eslint-parser": "^6.0.4",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vue-template-es2015-compiler": "1.8.2"
  }
}
