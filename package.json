{
  "name": "vue-fa",
  "version": "2.0.1",
  "description": "Tiny FontAwesome 5 component for Vue.js",
  "keywords": [
    "fontawesome",
    "font-awesome",
    "vue",
    "vue.js",
    "component",
    "icon",
    "svg",
    "duotone"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cweili/vue-fa.git"
  },
  "author": "Cweili",
  "license": "MIT",
  "homepage": "https://cweili.github.io/vue-fa/",
  "bugs": {
    "url": "https://github.com/Cweili/vue-fa/issues"
  },
  "main": "dist/vue-fa.js",
  "module": "dist/vue-fa.esm.js",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "postbuild": "eslint --fix *.js src/*.vue test/*.js docs/*.js docs/*.vue",
    "build:docs": "cd docs && rollup -c",
    "dev:build": "cd docs && rollup -c -w",
    "dev:serve": "sirv docs --dev",
    "dev": "run-p dev:build dev:serve",
    "test": "jest test --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@cweili/fa-test-util": "^0.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.0",
    "eslint-plugin-vue": "^6.0.0",
    "jest": "^26.0.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-vue": "^3.0.0",
    "sirv-cli": "^0.4.2",
    "vue": "^2.6.10",
    "vue-eslint-parser": "^7.0.0",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vue-template-es2015-compiler": "1.8.2"
  }
}
